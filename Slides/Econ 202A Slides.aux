\@writefile{toc}{\contentsline {paragraph}{If $(S,P)$ is a complete metric space and $T:S \rightarrow S$ is a contraction mapping with modulus $\beta $, then a $T$ has exactly one fixed point $v \in S$ and b for any $v_0 \in S$, $P(T^nV_0,V) \leq \beta ^n P(v_0,v)$ for $n = 0,1,2,...$}{5}{}\protected@file@percent }
